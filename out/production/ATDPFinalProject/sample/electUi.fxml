<GridPane hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
   
       <Button onAction="#energyClick" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="669.0" fitWidth="520.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/electricity.png" />
               </image>
            </ImageView>
         </graphic></Button>
       <Label fx:id="energyMeter" alignment="CENTER" contentDisplay="CENTER" prefHeight="17.0" prefWidth="393.0" text=" " GridPane.rowIndex="1" />
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
</GridPane>
